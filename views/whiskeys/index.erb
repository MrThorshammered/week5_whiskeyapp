<h2>Whiskeys</h2>

<a class="btn" href="/whiskeys/new">Add Whiskey</a>

<div class="Whiskey">
  <% @whiskeys.each do |whiskey| %>
  <div class="row">
    <img src="<%= whiskey.photo_url %>" alt="<%= whiskey.name %> photo">
    <ul>
      <li>
        <h2>
          <a href="/whiskeys/<%= whiskey.id %>">
            <%= whiskey.name %>
          </a>
        </h2>
      </li>
      <li>
        <a href="/whiskeys/<%= whiskey.id %>/edit">
          Edit
        </a>
      </li>
      <li>
        <li>
          <form action="/whiskeys/<%= whiskey.id %>/delete" method="post">
            <button>Delete</button>
          </form>
        </li>
      </li>
    </ul>
  </div>
  <% end %>
</div>
